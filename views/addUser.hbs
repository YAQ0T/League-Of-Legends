<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/stylesheets/addUser.css" />
    <title>Document</title>
  </head>
  <body>
    <h1>YAQOT Site welcomes you</h1>
    <dev class="main">
      <div class="create">
        <form action="/upload" method="POST" enctype="multipart/form-data">
          <p>User Name:</p>
          <input type="text" id="userName" name="userName" />

          <p>Password:</p>
          <input type="password" id="password" name="password" />

          <p>Age:</p>
          <input type="number" id="age" name="age" />

          <p>Description:</p>
          <textarea id="description" name="description"></textarea>

          <p>Link For User Image:</p>
          <input type="text" id="titlePhoto" name="titlePhoto" />

          <p>
            Link For Big Image:</p>
          <input type="text" id="bigPhoto" name="bigPhoto" />

          <input class="submit" type="submit" value="Sign Up" />
        </form>
      </div>
      <div class="or">|<br />|<br />|<br />|<br />|<br />|<br />|<br />|<br
        /><br />OR<br /><br />|<br />|<br />|<br />|<br />|<br />|<br />|<br
        />|<br />|<br /></div>
      <div class="login">
        <form action="/login" method="POST" enctype="multipart/form-data">
          <div class="inside">
            <p>User Name:</p>
            <input type="text" id="userName" name="userName" />
            <p>Password:</p>
            <input type="password" id="name" name="name" />
            <input class="submit" type="submit" value="Log In" />
          </div>

        </form>
      </div>
    </dev>
  </body>
</html>